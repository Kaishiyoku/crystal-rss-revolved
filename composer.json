{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "diglactic/laravel-breadcrumbs": "^9.0",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^1.0",
        "intervention/image": "^3.3",
        "kaishiyoku/hera-rss-crawler": "^6.0",
        "kornrunner/blurhash": "^1.2",
        "laravel/framework": "^11.0",
        "laravel/horizon": "^5.23",
        "laravel/pulse": "^1.2",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "neitanod/forceutf8": "^2.0",
        "predis/predis": "^2.2",
        "robertboes/inertia-breadcrumbs": "^0.6.0",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10",
        "barryvdh/laravel-ide-helper": "^3.0",
        "doctrine/dbal": "^3.9",
        "fakerphp/faker": "^1.23",
        "fumeapp/modeltyper": "^2.8",
        "larastan/larastan": "^2.8",
        "laravel-lang/common": "^3.1",
        "laravel/breeze": "^2.0",
        "laravel/envoy": "^2.9",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.23",
        "pestphp/pest-plugin-laravel": "^2.2",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write --reset"
        ],
        "analyze": "./vendor/bin/phpstan analyse --memory-limit=256M",
        "pint": "./vendor/bin/pint",
        "deploy": "./vendor/bin/envoy run deploy --ansi",
        "generate-typescript": "php artisan model:typer --no-hidden --use-enums  > resources/js/types/generated/models.d.ts && php -r \"file_put_contents(__DIR__ . '/resources/js/types/generated/models.d.ts', '/* eslint no-use-before-define: [\\'off\\'] */' . PHP_EOL . PHP_EOL . file_get_contents(__DIR__ . '/resources/js/types/generated/models.d.ts'));\" && npx eslint resources/js/types/generated/models.d.ts  --fix"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
