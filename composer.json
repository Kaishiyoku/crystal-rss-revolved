{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "diglactic/laravel-breadcrumbs": "^10.0",
        "guzzlehttp/guzzle": "^7.8",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image": "^3.3",
        "kaishiyoku/hera-rss-crawler": "^7.0",
        "kornrunner/blurhash": "^1.2",
        "laravel/framework": "^12.9",
        "laravel/horizon": "^5.30",
        "laravel/pulse": "^1.2",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "neitanod/forceutf8": "^2.0",
        "predis/predis": "^2.2",
        "robertboes/inertia-breadcrumbs": "^0.8.0",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "doctrine/dbal": "^4.0",
        "fakerphp/faker": "^1.23",
        "fumeapp/modeltyper": "^3.0",
        "larastan/larastan": "^3.1",
        "laravel-lang/common": "^6.0",
        "laravel/breeze": "^2.3",
        "laravel/envoy": "^2.10",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "spatie/laravel-ignition": "^2.9",
        "laravel/pail": "^1.2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write --reset"
        ],
        "analyze": "./vendor/bin/phpstan analyse --memory-limit=256M",
        "pint": "./vendor/bin/pint",
        "deploy": "./vendor/bin/envoy run deploy --ansi",
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
